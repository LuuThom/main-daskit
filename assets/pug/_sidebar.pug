    each val in document
      li.nav-item
        a.nav-link(class= val.sub ? "nav-link-sub": "" href="#" + val.link data-toggle="collapse" role="button" aria-expanded="true" aria-controls= val.link)
          i(class= val.icon)
          |#{val.item}
          if val.badge
            span.badge.version= val.badge
        if val.sub
