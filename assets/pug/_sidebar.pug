mixin navbar-nav()
  ul.navbar-nav
    each val in navbar
      li.nav-item
        a.nav-link(class= val.sub ? "nav-link-sub": "", href= "#" + val.link,
          data-toggle="collapse" 
          role="button" 
          aria-expanded= val.show ? "true" : "false" 
          aria-controls= val.link)
          i(class= val.icon)
          |#{val.item}
        if val.sub
          .collapse(
            id= val.link
            class= val.show ? "show" : ""
          )
            ul.nav.nav-sm.flex-column
              each val1 in val.sub
                li.nav-item
                  a.nav-link(class= val1.badge ? "nav-active": "", class= val1.child ? "nav-link-sub": "",href="#" + val1.link,
                    data-toggle="collapse" 
                    role="button" 
                    aria-expanded="false" 
                    aria-controls= val1.link)= val1.list
                    if val1.badge
                      span.badge= val1.badge
                  if val1.child
                    .collapse(id= val1.link)
                      ul.nav.nav-child.nav-sm.flex-column
                        each val2 in val1.child
                          li.nav-item
                            a.nav-link(href="#")= val2.list

